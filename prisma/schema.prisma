generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id        String   @id @default(cuid()) @unique
  name      String?
  demand    Int
  custValue Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
}

model Location {
  id        String   @id @default(cuid()) @unique
  name      String?
  fixedCost   Int
  capacity    Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model LocationClient {
  id        String   @id @default(cuid()) @unique
  locationId String
  clientId String
  cost Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Results {
  id       String   @id @default(cuid()) @unique
  name    String?
  result Float
  solutionData String
  inputdata String
  console String
  createdAt DateTime @default(now())
  maxMemory Float
  statusSolution String
  processingTime Float
}